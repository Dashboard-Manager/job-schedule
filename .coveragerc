[run]
branch = True
source = .
omit =
    isort/_vendored/*
    except ImportError:
    tests/*
    isort/deprecated/*

[report]
include = tests/*
exclude_lines =
    pragma: no cover
    except ImportError:
    if TYPE_CHECKING:
    raise NotImplementedError

[paths]
source = isort/
  .tox/*/lib/python*/site-packages/isort/
  .tox\*\Lib\site-packages\isort\